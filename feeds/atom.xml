---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<link rel="hub" href="https://pubsubhubbub.appspot.com"/>
	<title>Myles Braithwaite</title>
	<link href="http://mylesbraithwaite.com/atom.xml" rel="self"/>
	<link href="http://mylesbraithwaite.com/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://mylesbraithwaite.com/</id>
	<author>
		<name>Myles Braithwaite</name>
		<email>me@mylesbraithwaite.com</email>
	</author>
	
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title | xml_escape }}</title>
		{% if post.link %}
		<link href="{{ post.link }}"/>
		{% else %}
		<link href="http://mylesbraithwaite.com{{ post.url }}"/>
		{% endif %}
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://mylesbraithwaite.com{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
